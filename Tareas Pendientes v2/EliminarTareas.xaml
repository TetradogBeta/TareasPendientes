<Window x:Class="Tareas_Pendientes_v2.EliminarTareas"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tareas_Pendientes_v2"
        mc:Ignorable="d"
        Title="EliminarTareas" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="7*"/>
            <RowDefinition Height="77*"/>
            <RowDefinition Height="6*"/>
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0" x:Name="txblNombreLista"/>
        <StackPanel Grid.Row="1" x:Name="stkTareasLista"/>
        <CheckBox Grid.Row="2" x:Name="ckOmitirPregunta" Content="Omitir pregunta de seguridad"/>
    </Grid>
</Window>
